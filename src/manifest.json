{
  "name": "LibRedirect",
  "description": "__MSG_extensionDescription__",
  "version": "1.4.8",
  "manifest_version": 3,
  "background": {
    "service_worker": "pages/background/background.js",
    "type": "module"
  },
  "default_locale": "en",
  "icons": {
    "16": "assets/images/libredirect-16.png",
    "32": "assets/images/libredirect-32.png",
    "48": "assets/images/libredirect-48.png",
    "128": "assets/images/libredirect-128.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "LibRedirect",
    "browser_style": false,
    "default_popup": "pages/popup/popup.html",
    "default_icon": {
      "16": "assets/images/libredirect-16.png",
      "32": "assets/images/libredirect-32.png",
      "48": "assets/images/libredirect-48.png",
      "128": "assets/images/libredirect-128.png"
    }
  },
  "options_ui": {
    "page": "pages/options/general/general.html",
    "browser_style": false,
    "open_in_tab": true
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/javascripts/helpers/youtube/piped-preferences.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "assets/javascripts/helpers/youtube/pipedMaterial-preferences.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}